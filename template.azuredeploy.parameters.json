{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "https://raw.githubusercontent.com/ros-kamach/azure_openshift-origin_deploymen/azure_deployment"
        },
		"masterVmSize": {
			"value": "Standard_D4s_v3"
		},
		"infraVmSize": {
			"value": "Standard_D2s_v3"
		},
		"nodeVmSize": {
			"value": "Standard_D2s_v3"
		},
		"storageKind": {
			"value": "managed"
		},
        "openshiftClusterPrefix": {
            "value": "CLUSTER_PREFIX"
        },
        "masterInstanceCount": {
            "value": 1
        },
        "infraInstanceCount": {
            "value": 1
        },
        "nodeInstanceCount": {
            "value": 1
        },
        "dataDiskSize": {
            "value": 128
        },
        "adminUsername": {
            "value": "ADMIN_USER"
        },
        "openshiftPassword": {
            "value": "OPENSHIFT_PASS"
        },
		"enableMetrics": {
			"value": "false"
		},
		"enableLogging": {
			"value": "false"
		},
        "sshPublicKey": {
            "value": "SSH_PUBLIC_KEY"
        },
        "keyVaultResourceGroup": {
            "value": "RESOURCE_GROUP"
        },
        "keyVaultName": {
            "value": "KEYVAULT_NAME"
        },
        "keyVaultSecret": {
            "value": "KEYVAULT_SECRET_NAME"
        },
        "enableAzure": {
			"value": "true"
	    },
        "aadClientId": {
            "value": "APPID"
        },
        "aadClientSecret": {
            "value": "SP_PASSWORD"
        },
        "defaultSubDomainType": {
            "value": "nipio"
        },
        "defaultSubDomain": {
            "value": "example.com"
        }
    }
}
